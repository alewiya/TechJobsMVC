<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

    
    @if (ViewBag.jobs != null)
    {

        <h3>@ViewBag.jobs.Count Result(s)</h3>

        @foreach (var job in ViewBag.jobs)
        {

            @foreach (var row in job)
            {
                <table class="job-listing">
                    <thead>
                        <tr>

                            <th scope="col">
                                @row.Key

                            </th>
                            <th scope="col"> @row.Value</th>
                        </tr>
                    </thead>

                </table>
            }
        }
    }